<ap:domain xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:ap="http://labkey.org/study/assay/xml">
    <exp:Description>Species Specific PCR Results</exp:Description>
    <exp:PropertyDescriptor>
        <exp:Name>Date</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#dateTime</exp:RangeURI>
        <exp:Label>Measurement Date</exp:Label>
        <exp:Format>EEE, MMM d, yyyy</exp:Format>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ExpID</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Experiment ID</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ParticipantID</exp:Name>
        <exp:Description>Participant ID</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Patient</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Scientist</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Scientist Name</exp:Label>
        <exp:FK>
            <exp:Schema>core</exp:Schema>
            <exp:Query>users</exp:Query>
        </exp:FK>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>FreezerProID</exp:Name>
        <exp:Description>Enter all FreezerPro IDs of DNA used in PCR</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>FreezerPro IDs</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Attempt</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Attempt Number</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Sample</exp:Name>
        <exp:Description>Amount of sample used in PCR (uL)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Sample Amount</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>DNADilution</exp:Name>
        <exp:Description>DNA dilution value</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>DNA Dilution</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>PfPresent</exp:Name>
        <exp:Description>Pf present - single step method (y/n)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#boolean</exp:RangeURI>
        <exp:Label>Pf Present</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>PfBand</exp:Name>
        <exp:Description>If Pf present - approximate beand size</exp:Description>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Pf Band Size</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>PvPresent</exp:Name>
        <exp:Description>Pv present - single step method (y/n)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#boolean</exp:RangeURI>
        <exp:Label>Pv Present</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>PvBand</exp:Name>
        <exp:Description>If Pv present - approximate band size</exp:Description>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Pv Band Size</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>GelImage</exp:Name>
        <exp:Description>Gel image upload (.jpeg file)</exp:Description>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Gel Image</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>FinalAttempt</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#boolean</exp:RangeURI>
        <exp:Label>Final Attempt</exp:Label>
    </exp:PropertyDescriptor>
</ap:domain>