<ap:domain xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:ap="http://labkey.org/study/assay/xml">
    <exp:Description>Diagnostics Assay Results</exp:Description>
    <exp:PropertyDescriptor>
        <exp:Name>Date</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#dateTime</exp:RangeURI>
        <exp:Label>Measurement Date</exp:Label>
        <exp:Format>EEE, MMM d, yyyy</exp:Format>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Time</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#dateTime</exp:RangeURI>
        <exp:Label>Measurement Time</exp:Label>
        <exp:Format>h:mm a</exp:Format>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Scientist</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Scientist Name</exp:Label>
        <exp:FK>
            <exp:Schema>core</exp:Schema>
            <exp:Query>users</exp:Query>
        </exp:FK>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ParticipantID</exp:Name>
        <exp:Description>Participant ID</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Patient</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ProcessingProtocol</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Processing Protocol</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>InitPvParasitemia</exp:Name>
        <exp:Description>Initial Pv Parasitemia percentage</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Initial Pv Parasitemia</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>InitPfParasitemia</exp:Name>
        <exp:Description>Initial Pf Parasitemia percentage</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Initial Pf Parasitemia</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>InitPmParasitemia</exp:Name>
        <exp:Description>Initial Pm Parasitemia percentage</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Initial Pm Parasitemia</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ParasiteDensity</exp:Name>
        <exp:Description>Parasite Density (parasites/uL)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Parasite Density</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>InitGametocytemia</exp:Name>
        <exp:Description>Initial Gametocytemia percentage</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Initial Gametocytemia</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>GametocyteDensity</exp:Name>
        <exp:Description>Gametocyte Density (gametocytes/uL)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Gametocyte Density</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>PatientHemoglobin</exp:Name>
        <exp:Description>Patient Hemoglobin (g/dL)</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Patient Hemoglobin</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Hematocrit</exp:Name>
        <exp:Description>Hematocrit percentage</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Hematocrit</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ThickBloodSmear</exp:Name>
        <exp:Description>Thick blood smear species identification</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Thick blood</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>ThinBloodSmear</exp:Name>
        <exp:Description>Thin blood smear species identification</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Thin blood</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>RDT</exp:Name>
        <exp:Description>RDT species identification</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>RDT</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>FreezerProID</exp:Name>
        <exp:Description>Add all FreezerPRO IDs for this sample</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>FreezerPro IDs</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>BacteriaSeen</exp:Name>
        <exp:Description>Bacteria seen in slide</exp:Description>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Bacteria seen in slide</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Comments</exp:Name>
        <exp:Description>Comments</exp:Description>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Comments</exp:Label>
    </exp:PropertyDescriptor>
</ap:domain>