<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Runs" tableDbType="TABLE">
                <columns>
                    <column columnName="DailyMaintenance" wrappedColumnName="rowId">
                        <columnTitle></columnTitle>
                        <fk>
                            <fkTable>runs_dailyupload</fkTable>
                            <fkColumnName>rowId</fkColumnName>
                        </fk>
                        <url>/icemr/DailyUpload.view?rowId=${RowId}&amp;protocolId=${Batch/BatchProtocolId/RowId}&amp;batchId=${Batch/RowId}</url>
                    </column>
                </columns>
                <buttonBarOptions position="both" includeStandardButtons="true">
                        <item insertPosition="end" text="New Experiment" >
                                <originalText>Import Data</originalText>
                        </item>
                </buttonBarOptions>
            </table>
        </tables>
    </metadata>
</query>
